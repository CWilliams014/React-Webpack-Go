{
  "name": "webpack-starter-setup",
  "version": "1.0.0",
  "description": "webpack setup boilerplate",
  "main": "webpack-production.config.js",

  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack -p", 
    "dev": "webpack-dev-server --env.dev --hot",
    "build:dev" : "webpack",
    "validate": "npm run lint",
    "start": "nodemon --watch server --exec babel-node -- server/server.js",
    "lint": "node_modules/.bin/eslint webpack.config.* client",
    "test": "karma start",
    "watch:test": "npm test -- --auto-watch --no-single-run",
  },

  "author": "Cole Williams",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-optimize": "^1.0.1",
    "css-loader": "^0.25.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "jshint": "^2.9.3",
    "jshint-loader": "^0.8.3",
    "karma": "1.1.2",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-mocha": "1.1.1",
    "karma-webpack": "1.7.0",
    "mocha": "3.0.1",
    "node-libs-browser": "^1.0.0",
    "nodemon": "^1.10.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^1.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.20",
    "webpack-config-utils": "2.0.0",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-validator": "2.2.7",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "react": "^15.3.1"
  }
}


